/**
 * Not gate:
 * out = not in
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=in,b=in, out=out);
    // 0 -> 1
    // 1 -> 0
    // 1 nand 1 -> 0 
    // 0 nand 0 -> 1
    
}